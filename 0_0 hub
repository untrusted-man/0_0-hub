        InfoDescription.Text = Languages[HubState.CurrentLanguage].version_info or "Hub Version: 2.2 Ultimate (Updated 2025)\nLast Updated: September 21, 2025\nScripts Available: 35+\nPremium Features: Unlocked\n\n‚ú® Features:\n‚Ä¢ Modern glassmorphism UI\n‚Ä¢ 8 language support\n‚Ä¢ 12 premium themes\n‚Ä¢ Advanced script library\n‚Ä¢ Performance monitoring\n‚Ä¢ Custom hotkey system\n‚Ä¢ Auto-updater\n‚Ä¢ Advanced ESP\n‚Ä¢ Particle effects\n‚Ä¢ Sound effects\n\nüíé Thank you for using 0_0 Hub Ultimate!"
 
        InfoDescription.Font = Enum.Font.Gotham
