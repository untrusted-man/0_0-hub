        ThemeCorner.CornerRadius = UDim.new(0, 12)
        ThemeCorner.Parent = ThemeSection
        
        local ThemeTitle = Instance.new("TextLabel")
        ThemeTitle.Size = UDim2.new(1, 0, 0, 30)
        ThemeTitle.Position = UDim2.new(0, 20, 0, 10)
