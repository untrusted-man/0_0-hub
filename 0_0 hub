        LanguageTitle.BackgroundTransparency = 1
    
        LanguageTitle.Text = Languages[HubState.CurrentLanguage].language_label or "Language"
        LanguageTitle.Font = Enum.Font.GothamBold
        LanguageTitle.TextSize = 20
